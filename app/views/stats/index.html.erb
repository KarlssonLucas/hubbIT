
<table class="mdl-data-table mdl-js-data-table mdl-data-table--selectable mdl-shadow--2dp">
<% @total_time.each do |user| %>
<tr>
  <td>
      <%= link_to user.user_id, stats_path(user.user_id) %>
  </td>
  <td>
      <%= seconds_to_units(user.total_time) %>
  </td>
</tr>
<% end%>
</table>

<canvas id="chart" class="stats-graph" data-url="/stats/hours.json" width="1200" height="300"></canvas>
